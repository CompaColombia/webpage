ort { AiOutlineMenu } from 'react-icons/ai';\n\nfunction Navbar() {\n  const [openMenu, setOpenMenu] = useState(true);\n  console.log(openMenu);\n\n  const storeScroll = () => {\n    console.log(window.scrollY)\n    document.documentElement.dataset.scroll = window.scrollY;\n  };\n\n  // Listen for new scroll events\n  document.addEventListener('scroll', storeScroll);\n\n  // Update scroll position for first time\n  storeScroll();\n\n  useEffect(() => {\n    if (storeScroll === 0) {\n      setOpenMenu(true)\n    } else { setOpenMenu(false) }\n    console.log(storeScroll)\n    console.log(openMenu)\n    \n  }, window.scrollY);\n\n\n  return (\n    <>\n      <nav>\n        <Link to='/' className='navbar-logo'>\n          <img className='title-logo'\n            alt='Compa logo'\n            src='/images/compa negro.png'\n          />\n          <img className='title-logo-small'\n            alt='Compa logo'\n            src='/images/logo-small.png'\n          />\n        </Link>\n        <div className='menu-section' onClick={() => setOpenMenu(!openMenu)}>\n          {openMenu ?\n          <ul>\n            <li> <Link to='/agro' > Agro </Link> </li>\n            <li> <Link to='/tech' > Tecnología </Link> </li>\n            <li> <Link to='/edu' > Educación </Link> </li>\n            <li> <Link to='/mercado' > Mercado </Link> </li>\n          </ul> :\n          <AiOutlineMenu className='AiOutlineMenu' />\n          }\n        </div>\n      </nav>\n    </>\n  );\n}\n\nexport default Navbar;\n","/Users/harold/Documents/COMPA/webpage/compa-webpage/src/pages/Home.js",["94"],[],"import React from 'react';\nimport Cards from '../components/Cards';\nimport Team from '../components/Team';\nimport Allies from '../components/Allies';\nimport HeroSection from '../components/HeroSection';\n\nfunction Home() {\n  return (\n    <>\n      <HeroSection />\n      <Cards />\n      {/* <Team /> */}\n      <Allies />\n    </>\n  );\n}\n\nexport default Home;\n","/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/Modal.js",[],[],"/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/Table.js",[],[],"/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/ColombianMap.js",["95"],[],"import { useEffect, useState } from \"react\";\nimport MapSvg from \"./MapSvg\";\n\nconst ColombianMap = () => {\n\n  const [regionSelected, setRegionSelected] = useState({})\n  const [prevDepartment, setPrevDepartment] = useState(null)\n  const [title, setTitle] = useState('Selecciona un departamento')\n\n  useEffect(() => {\n    if (Object.keys(regionSelected).length !== 0) {\n\n      const department = document.getElementById(regionSelected.getAttribute('id'))\n      if (department !== null) department.style.fill = 'red'\n      if (prevDepartment !== null) prevDepartment.style.fill = 'black'\n      \n      setPrevDepartment(department);\n      setTitle(regionSelected.getAttribute('name') || title)\n    }\n  }, [regionSelected]);\n\n  return (\n    <div>\n      <h3>{title}</h3>\n      <MapSvg setRegionSelected={setRegionSelected} />\n    </div>\n  );\n}\n\nexport default ColombianMap;","/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/Allies.js",["96"],[],"import React from 'react';\nimport { Pagination } from \"swiper\";\n\nimport { Swiper, SwiperSlide } from \"swiper/react\";\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/pagination';\n\nfunction Allies() {\n\n  const array = [ \n    {\n      img: 'arroyave.png',\n      link: 'https://semillasarroyave.com/'\n    },\n    {\n      img: 'ball.png',\n      link: 'https://ballsb.com/es/nosotros'\n    },\n    {\n      img: 'fee.png',\n      link: 'https://www.feecolombia.org/'\n    },\n    {\n      img: 'rioplant.jpeg',\n      link: 'https://rioplant.com/'\n    },\n    {\n      img: 'saenzfety.png',\n      link: 'https://saenzfety.com/'\n    },\n  ]\n\n  return (\n    <div className='allies'>\n      <h1>Los compas de Compa</h1>\n      <Swiper\n        slidesPerView={3}\n        spaceBetween={30}\n        pagination={{\n          clickable: true,\n        }}\n        modules={[Pagination]}\n        className=\"myswiper\"\n      >\n        {array.map((item, key) => {\n          return <SwiperSlide key={key} >\n            <a href={item.link} target=\"_blank\">\n            <img \n              alt={`logo de ${item.img}`}\n              src={`allies/${item.img}`}  \n            />\n            </a>\n          </SwiperSlide>\n        })}\n      </Swiper>\n    </div>\n  );\n}\n\nexport default Allies;\n","/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/Team.js",[],[],"/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/Cards.js",[],[],"/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/HeroSection.js",[],[],"/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/MapSvg.js",[],[],"/Users/harold/Documents/COMPA/webpage/compa-webpage/src/components/CardItem.js",["97"],[],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\nfunction CardItem(props) {\n  return (\n    <>\n      <li className='cards__item'>\n        <Link className='cards__item__link' to={props.path}>\n          <figure className='cards__item__pic-wrap' style={{'--bg-color': props.bgColor}} data-category={props.label}>\n            <img\n              className='cards__item__img'\n              alt='Travel Image'\n              src={props.src}\n            />\n          </figure>\n          {props.text && <div className='cards__item__info'>\n            <h5 className='cards__item__text'>{props.text}</h5>\n          </div>}\n        </Link>\n      </li>\n    </>\n  );\n}\n\nexport default CardItem;\n",{"ruleId":"98","severity":1,"message":"99","line":27,"column":6,"nodeType":"100","endLine":27,"endColumn":20},{"ruleId":"98","severity":1,"message":"101","line":27,"column":6,"nodeType":"100","endLine":27,"endColumn":20,"suggestions":"102"},{"ruleId":"103","severity":1,"messag